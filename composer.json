{
    "name": "nddcoder/laravel-simple-viewable",
    "description": "Simple viewable for eloquent",
    "keywords": [
        "nddcoder",
        "laravel-simple-viewable"
    ],
    "homepage": "https://github.com/nddcoder/laravel-simple-viewable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dung Nguyen Dang",
            "email": "dangdungcntt@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/database": "5.5.*|5.6.*|5.7.*|5.8.*",
        "jaybizzle/crawler-detect": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Nddcoder\\LaravelSimpleViewable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nddcoder\\LaravelSimpleViewable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nddcoder\\LaravelSimpleViewable\\LaravelSimpleViewableServiceProvider"
            ],
            "aliases": {
                "LaravelSimpleViewable": "Nddcoder\\LaravelSimpleViewable\\LaravelSimpleViewableFacade"
            }
        }
    }
}
